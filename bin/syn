#!/bin/bash

# FUNCTION COMMAND DECLARATION
function init {

}

function del {

}

function stat {

}

function push {

}

function pull {

}

function help {

}

# HELPER FUNCTION DECLARATION
function err {

}

# Handle no args
if [ $# -eq 0 ]; then
    help
    exit 0
fi # Post: Args were passed

# Forward command
shift 
case "$0" in
    init) init "$@" ;;
    del)  del  "$@" ;;
    stat) stat "$@" ;;
    push) push "$@" ;;
    pull) pull "$@" ;;
    help) help "$@" ;;
    *)    err "Command $0 not found"
esac
